<p id="notice"><%= notice %></p>

<h1>Deployments</h1>

<table>
  <thead>
    <tr>
      <th>Bearid</th>
      <th>Ptt id</th>
      <th>Capture lat</th>
      <th>Capture long</th>
      <th>Sex</th>
      <th>Age class</th>
      <th>Ear applied</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @deployments.each do |deployment| %>
      <tr>
        <td><%= deployment.BearID %></td>
        <td><%= deployment.PTT_ID %></td>
        <td><%= deployment.capture_lat %></td>
        <td><%= deployment.capture_long %></td>
        <td><%= deployment.Sex %></td>
        <td><%= deployment.Age_class %></td>
        <td><%= deployment.Ear_applied %></td>
        <td><%= link_to 'Show', deployment %></td>
        <td><%= link_to 'Edit', edit_deployment_path(deployment) %></td>
        <td><%= link_to 'Destroy', deployment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Deployment', new_deployment_path %>
